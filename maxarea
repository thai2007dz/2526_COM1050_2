class Solution:
    def maxArea(self, height: List[int]) -> int:
        left=0
        right=len(height)-1
        max=0
        while left!=right:
            if height[left]<height[right]:
                if max<height[left]*(right-left):
                    max=height[left]*(right-left)
                left+=1
            else:
                if max<height[right]*(right-left):
                    max=height[right]*(right-left)
                right-=1
        return max
